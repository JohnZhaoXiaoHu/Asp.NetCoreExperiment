{
  "$schema": "http://json.schemastore.org/template",
  "author": "桂素伟",
  "classifications": [ "Common", "桂素伟模板", "WebAPI", "C#9" ],
  "identity": "GSW  Nuget",
  "name": "GSW  Nuget",
  "shortName": "GSWNuget",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "GSWNuget",
  "symbols": {
    "MyName": {
      "type": "parameter",
      "datatype": "text",
      "defaultValue": "",
      "replaces": "myname",
      "description": "我的名称"
    },

    "UseAuth": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "true",
      "description": "是否使用Auth"
    },
    "UseMyData": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "true",
      "description": "是否使用UseMyData"
    },
    "UseNLog": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "true",
      "description": "是否使用NLog"
    },
    "UseDapper": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "true",
      "description": "是否使用Dapper"
    },
    "BackgroundColor": {
      "type": "parameter",
      "datatype": "choice",
      "defaultValue": "aliceblue",
      "choices": [
        {
          "choice": "aliceblue",
          "description": "Alice Blue"
        },
        {
          "choice": "dimgray",
          "description": "dimgray"
        },
        {
          "choice": "skyblue",
          "description": "skyblue"
        }
      ],
      "replaces": "skyblue"
    },
    "Aliceblue": {
      "type": "computed",
      "value": "(BackgroundColor == \"aliceblue\")"
    },
    "Dimgray": {
      "type": "computed",
      "value": "(BackgroundColor == \"dimgray\")"
    },
    "Dkyblue": {
      "type": "computed",
      "value": "(BackgroundColor == \"skyblue\")"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "condition": "(!UseMyData)",
          "exclude": [ "MyData/Data.cs" ]
        }
      ]
    }
  ]  
}